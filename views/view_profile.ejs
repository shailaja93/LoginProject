<table border=1>
<tr><th>Username</th>
<th>First Name</th>
<th>Last Name</th>
<th>Address1</th>
<th>Address2</th>
</tr>
<% userinfo.forEach(function (users) { %>
<tr>
<td><%= users.username %></td>
<td><%= users.fname %></td>
<td><%= users.lname %></td>
<td><%= users.address1 %></td>
<td><%= users.address2 %></td>
</tr>
<% }); %>
</table>